  "license": "MIT",
  "dependencies": {
    "elm": "^0.19.1-5",
    "parcel": "^2.5.0"
  },
  "devDependencies": {
    "@dillonkearns/elm-graphql": "^3.2.0",
    "@parcel/packager-raw-url": "2.5.0",
