    "@parcel/transformer-webmanifest": "2.5.0",
    "elm-analyse": "^0.16.5",
    "elm-format": "^0.8.5",
    "elm-hot": "^1.0.1",
    "elm-test": "^0.19.1-revision7",
    "node-elm-compiler": "^5.0.3"
  },
  "scripts": {
    "api": "elm-graphql http://localhost:8000/graphql --base Gallery --output gen",
